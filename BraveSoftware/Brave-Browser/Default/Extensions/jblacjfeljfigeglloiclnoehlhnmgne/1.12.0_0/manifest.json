      "js": [ "content-yaps.js" ],
      "matches": [ "https://yaps.kaito.ai/*" ]
   }, {
      "css": [ "styles-opensea.css" ],
      "js": [ "content-opensea.js" ],
      "matches": [ "https://opensea.io/*" ]
   }, {
      "css": [ "styles-farcaster.css" ],
      "js": [ "content-farcaster.js" ],
      "matches": [ "https://farcaster.xyz/*" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "View Ethos credibility scores and data to enhance your web3 browsing experience, ensuring you always know whoâ€™s trustworthy in web3.",
   "host_permissions": [ "https://x.com/*", "https://twitter.com/*", "https://api.ethos.network/*" ],
