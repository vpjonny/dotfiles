{
   "action": {
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Alt+Shift+B"
         }
      }
   },
   "content_scripts": [ {
      "exclude_globs": [ "*chrome-extension*", "*.google.com*" ],
      "exclude_matches": [ "*://*.linear.app/*", "*://*.apple.com/*", "*://*.axiom.co/*", "*://*.tradingview.com/*", "*://*.dropbox.com/*", "*://*.google.com/*", "*://*.github.com/*", "*://*.cloudflare.com/*", "*://*.intuit.com/*" ],
      "js": [ "inpage-solana-early.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start",
      "world": "MAIN"
   }, {
      "all_frames": true,
      "exclude_globs": [ "*chrome-extension*", "*.google.com*" ],
      "exclude_matches": [ "*://*.linear.app/*", "*://*.apple.com/*", "*://*.axiom.co/*", "*://*.tradingview.com/*", "*://*.dropbox.com/*", "*://*.google.com/*", "*://*.github.com/*", "*://*.cloudflare.com/*", "*://*.intuit.com/*" ],
      "js": [ "contentScript-early-evm.js", "contentScript-early-solana.js", "contentScript.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "js": [ "vendor/trezor-content-script.js" ],
      "matches": [ "https://connect.trezor.io/*" ]
   } ],
   "description": "A next level crypto wallet for Solana, Ethereum, Monad, Sui, and more.",
   "host_permissions": [ "https://twitter.com/*", "https://x.com/*", "https://connect.trezor.io/*" ],
   "icons": {
      "16": "anchor.png",
      "192": "anchor.png",
      "512": "anchor.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Backpack",
   "permissions": [ "storage", "unlimitedStorage", "background", "sidePanel", "declarativeNetRequest" ],
   "side_panel": {
      "default_path": "sidePanel.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.10.190",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "injected.js", "inpage-evm-early.js", "inpage-solana-early.js" ]
   } ]
}
